<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Website_VR</fullName>
    <active>false</active>
    <errorConditionFormula>if(REGEX(Website ,&quot;^((http|https)://)??(www[.])??([a-zA-Z0-9]|-)+?([.][a-zA-Z0-9(-|/|=|?)??]+?)+?$&quot;),false,true)</errorConditionFormula>
    <errorDisplayField>Website</errorDisplayField>
    <errorMessage>Enter a valid Website</errorMessage>
</ValidationRule>
